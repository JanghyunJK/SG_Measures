          <display_name>ALL</display_name>
        </choice>
        <choice>
          <value>NONE</value>
          <display_name>NONE</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>debug_mode</name>
      <display_name>Debug Mode</display_name>
      <description>Print debugging info</description>
      <type>Boolean</type>
      <required>false</required>
