      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>NONE</default_value>
      <choices>
        <choice>
          <value>N</value>
          <display_name>N</display_name>
        </choice>
        <choice>
          <value>E</value>
          <display_name>E</display_name>
        </choice>
        <choice>
          <value>S</value>
