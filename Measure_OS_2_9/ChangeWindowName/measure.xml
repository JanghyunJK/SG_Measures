  <class_name>ChangeWindowConstruction</class_name>
  <display_name>Change construction name in fenestration surfaces</display_name>
  <description>changing the construction name assigned in FenestrationSurface:Detailed objects to the specified argument string.</description>
  <modeler_description>this is to use with Inject_IDF measure to assign correct window construction names when glazing materials are imported via Inject_IDF measure.</modeler_description>
  <arguments>
    <argument>
      <name>choice</name>
      <display_name>Choice of window.</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>All fenestration surfaces</default_value>
      <choices>
        <choice>
          <value>All fenestration surfaces</value>
          <display_name>All fenestration surfaces</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>name_construction</name>
      <display_name>Window construction name that is imported through Inject_IDF measure.</display_name>
      <type>String</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>GlzSys_6</default_value>
    </argument>
  </arguments>
